{
  "name": "laudos",
  "productName": "Sistema de Laudos Médicos",
  "description": "Sistema de Laudos Médicos",
  "version": "2.2.0",
  "author": {
    "name": "Eddwan Hallen",
    "email": "eddwan@gmail.com",
    "url": "https://github.com/eddwan"
  },
  "repository": "https://github.com/eddwan/laudos",
  "copyright": "© 2019, Eddwan Hallen",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "build:electron": "npm run postinstall:electron && ng build --base-href ./",
    "build:electron:dev": "npm run build:electron -- -c dev",
    "build:electron:prod": "npm run build:electron -- -c production",
    "electron:dev": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "electron:serve": "wait-on http-get://localhost:4200/ && run-electron . --serve",
    "electron:local": "npm run build:electron:prod && run-electron .",
    "electron:linux": "npm run build:electron:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:electron:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:electron:prod && npx electron-builder build --mac",
    "ionic": "npm run ng:serve:web",
    "emulate:ios-dev": "node config/disable-deploy && node config/postinstall-web && ionic cordova emulate ios --l -- --buildFlag='-UseModernBuildSystem=0'",
    "run:ios-dev": "node config/disable-deploy && node config/postinstall-web && ionic cordova run ios --l --device -- --buildFlag='-UseModernBuildSystem=0'",
    "emulate:ios": "node config/disable-deploy && node config/postinstall-web && ionic cordova emulate ios -- --buildFlag='-UseModernBuildSystem=0'",
    "run:ios": "node config/disable-deploy && node config/postinstall-web && ionic cordova run ios --device -- --buildFlag='-UseModernBuildSystem=0'",
    "release:ios": "node config/enable-deploy && node config/postinstall-web && ionic cordova build ios --prod --release -- --buildFlag='-UseModernBuildSystem=0'",
    "emulate:android-dev": "node config/disable-deploy && node config/postinstall-web && ionic cordova emulate android --l",
    "run:android-dev": "node config/disable-deploy && node config/postinstall-web && ionic cordova run android --device --l",
    "emulate:android": "node config/disable-deploy && node config/postinstall-web && ionic cordova emulate android",
    "run:android": "node config/disable-deploy && node config/postinstall-web && ionic cordova run android --device",
    "release:android": "node config/enable-deploy && node config/postinstall-web && ionic cordova build android --prod --release ",
    "ng": "ng",
    "ng:update:all:force": "ng update --all --force",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node config/postinstall-web",
    "postinstall:electron": "node config/postinstall",
    "precommit": "node config/enable-deploy && git add config.xml",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "xcode": "open -a Xcode platforms/ios/"
  },
